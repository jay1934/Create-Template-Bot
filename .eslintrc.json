{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "global-require": "off",
    "no-unused-expressions": ["error", { "allowTernary": true }],
    "no-throw-literal": "off",
    "consistent-return": "off",
    "import/no-dynamic-require": "off",
    "import/prefer-default-export": "off",
    "no-shadow": "off",
    "no-bitwise": "off",
    "no-console": "off"
  }
}
